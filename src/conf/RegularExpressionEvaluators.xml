<evaluators>
    <evaluator token="CATALOGUEPREFIX" query-dependant="false">
        <pattern>telefon(nummer){0,1}\\s+(til|for){0,1}(\\s*\\:){0,1}</pattern>
        <pattern>tlf\\s+(til|for){0,1}(\\s*\\:){0,1}</pattern>
        <pattern>nummer\\s+(til|for){0,1}(\\s*\\:){0,1}</pattern>
        <pattern>adresse(n){0,1}\\s+(til|for){0,1}(\\s*\\:){0,1}</pattern>
        <pattern>Hvor(\\s+er){0,1}</pattern>
    </evaluator>
    <evaluator token="PHONENUMBER" query-dependant="false">
        <pattern>((\\+|00)47){0,1}\\s*(\\d\\s{0,1}){8}</pattern>
    </evaluator>
    <evaluator token="PICTUREPREFIX" query-dependant="true">
        <pattern>^bilde(r){0,1}\\s{0,1}(av){0,1}(\\s*\\:){0,1}</pattern>
        <pattern>^jpg(\\s*\\:){0,1}</pattern>
    </evaluator>
    <evaluator token="ORGNR" query-dependant="false">
        <pattern>//d{9}</pattern>
    </evaluator>
    <evaluator token="NEWSPREFIX" query-dependant="true">
        <pattern>^nyhet(er){0,1}(\\s+om){0,1}(\\s*\\:){0,1}</pattern>
        <pattern>^(siste\\s+){0,1}nytt(\\s+om){0,1}(\\s*\\:){0,1}</pattern>
        <pattern>^aviser(\\s*\\:){0,1}</pattern>
        <pattern>^nettaviser(\\s*\\:){0,1}</pattern>
    </evaluator>
    <evaluator token="WIKIPEDIAPREFIX" query-dependant="false">
        <pattern>wiki(pedia){0,1}(\\s*\\:){0,1}</pattern>
        <pattern>beskriv(\\s*\\:){0,1}</pattern>
        <pattern>leksikon(\\s*\\:){0,1}</pattern>
        <pattern>fakta(\\s*\\:){0,1}</pattern>
    </evaluator>
    <evaluator token="TVPREFIX" query-dependant="true">
        <pattern>^p.*\\s+tv\\s+(i\\s{0,1}dag){0,1}(\\s*\\:){0,1}</pattern>
        <pattern>^programoversikt(\\s*\\:){0,1}</pattern>
        <pattern>^program(\\s*\\:){0,1}</pattern>
        <pattern>^tv(-| )program(\\s*\\:){0,1}</pattern>
        <pattern>^tv(\\s*\\:){0,1}</pattern>
        <pattern>^fjernsyn(\\s*\\:){0,1}</pattern>
    </evaluator>
    <evaluator token="COMPANYSUFFIX" query-dependant="false">
        <pattern>\\sas(a){0,1}\\s*</pattern>
        <pattern>\\s&amp; co\\s*</pattern> <!-- maybe this one needs to be a CDATA ? -->
    </evaluator>
    <evaluator token="WEATHERPREFIX" query-dependant="true">
        <pattern>^regn(\\s*\\:){0,1}</pattern>
        <pattern>^v.*r(et|melding|varsel){0,1}\\s{0,1}(i|p.*|for){0,1}(\\s*\\:){0,1}</pattern>
        <pattern>^temperatur\\s{0,1}(i|p.*|for){0,1}(\\s*\\:){0,1}</pattern>
        <pattern>^varsel\\s{0,1}(i|p.*|for){0,1}(\\s*\\:){0,1}</pattern>
    </evaluator>
    <evaluator token="MATHPREDICATE" query-dependant="false">
        <pattern>[\\+\\-\\*\\/(]</pattern>
    </evaluator>
</evaluators>
