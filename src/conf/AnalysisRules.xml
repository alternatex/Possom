    Created on : 25 January 2006, 13:21
    Author     : <a href="mailto:mick@wever.org">Michael Semb Wever</a>
    Version    : $Id$
    Description:
        Purpose of the document follows.
-->

<rules>
    
    <!-- Global predicates -->
    <predicate id="geo">
        <any>
            <predicate id="GEOLOCAL"/>
            <predicate id="GEOGLOBAL"/>
        </any>
    </predicate>
    <predicate id="geoExact">
        <any>
            <predicate id="GEOLOCALEXACT"/>
            <predicate id="GEOGLOBALEXACT"/>
        </any>
    </predicate>
    <predicate id="exactFirstOrLast">
        <any>
            <predicate id="EXACTFIRST"/>
            <predicate id="EXACTLAST"/>
        </any>
    </predicate>
    <predicate id="firstAndLastName">
        <all>
            <predicate id="FIRSTNAME"/>
            <predicate id="LASTNAME"/>
        </all>
    </predicate>
    <predicate id="firstOrLast">
        <any>
            <predicate id="FIRSTNAME"/>
            <predicate id="LASTNAME"/>
        </any>
    </predicate>
    <predicate id="firstOrLastAndGeo">
        <any>
            <predicate id="firstOrLast"/>
            <predicate id="geo"/>
        </any>
    </predicate>
    <predicate id="notWikiNotCompanyPostfix">
        <none>
            <predicate id="EXACTWIKI"/>
            <predicate id="COMPANYSUFFIX"/>
            <predicate id="KEYWORD"/>
            <predicate id="CATEGORY"/>
            <predicate>
                <all>
                    <predicate id="PRIOCOMPANYNAME"/>
                    <predicate><not><predicate id="FIRSTNAME"/></not></predicate>
                </all>
            </predicate>
        </none>
    </predicate>
    <predicate id="firstOrLastNotCompany">
        <all>
            <predicate id="notWikiNotCompanyPostfix"/>
            <predicate id="firstOrLastAndGeo"/>
        </all>
    </predicate>
    <predicate id="catalogueAndName">
        <all>
            <predicate id="CATALOGUEPREFIX"/>
            <predicate id="firstOrLast"/>
        </all>
    </predicate>
    <predicate id="w">
        <all>
            <predicate id="notWikiNotCompanyPostfix"/>
            <predicate id="firstAndLastName"/>
        </all>
    </predicate>
    <predicate id="whiteBoost">
        <any>
            <predicate id="firstOrLastNotCompany"/>
            <predicate id="catalogueAndName"/>
            <predicate id="w"/>
        </any>
    </predicate>
    <predicate id="categoryOrKeyword">
        <any>
            <predicate id="CATEGORY"/>
            <predicate id="KEYWORD"/>
        </any>
    </predicate>
    <predicate id="englishWordsWithoutNorwegianStuff">
        <all>
            <predicate id="ENGLISHWORDS"/>
            <predicate>
                <none>
                    <predicate id="TNS"/>
                    <predicate id="EXACTWIKI"/>
                    <predicate id="geo"/>
                    <predicate id="KEYWORD"/>
                    <predicate id="CATEGORY"/>
                    <predicate id="firstOrLast"/>
                </none>
            </predicate>
        </all>
    </predicate>
    <predicate id="companyNotPerson">
            <all>
                <predicate id="COMPANYNAME"/>
                <predicate><not><predicate id="firstAndLastName"/></not></predicate>
            </all>
        </predicate>
    
    <!-- Rules -->
    
    <rule id="whitePages">
        
        <!-- private predicates -->
        <predicate id="personOtherPrefixes">
            <any>
                <predicate id="PICTUREPREFIX"/>
                <predicate id="WEATHERPREFIX"/>
                <predicate id="NEWSPREFIX"/>
                <predicate id="WIKIPEDIAPREFIX"/>
                <predicate id="TVPREFIX"/>
            </any>
        </predicate>
        
        <!-- Scoring -->
        <score predicate="FULLNAME">10</score>
        <score predicate="w">90</score>
        <score predicate="firstOrLastNotCompany">90</score>
        <score predicate="geoExact">-500</score>
        <score predicate="PHONENUMBER">150</score>
        <score predicate="catalogueAndName">150</score>
        <score predicate="personOtherPrefixes">-500</score>
        <score predicate="TNS">-500</score>
        <score predicate="exactFirstOrLast">-500</score>
    </rule>
    
    <rule id="yellowPages">
        
        <!-- private predicates -->
        
        
        <predicate id="gBanger">
            <all>
                <predicate id="EXACTWIKI"/>
                <predicate>
                    <none>
                        <predicate id="COMPANYSUFFIX"/>
                        <predicate id="KEYWORD"/>
                        <predicate id="CATEGORY"/>
                        <predicate id="PRIOCOMPANYNAME"/>
                    </none>
                </predicate>
            </all>
        </predicate>
        <predicate id="firstAndLastNameAndCompanySuffix">
            <all>
                <predicate id="COMPANYSUFFIX"/>
                <predicate id="firstAndLastName"/>
            </all>
        </predicate>
        <predicate id="allNotsAndFirstOrLastAndGeo">
            <all>
                <predicate id="firstOrLastAndGeo"/>
                <predicate><not><predicate id="PRIOCOMPANYNAME"/></not></predicate>
                <predicate><not><predicate id="COMPANYSUFFIX"/></not></predicate>
                <predicate><not><predicate id="categoryOrKeyword"/></not></predicate>
            </all>
        </predicate>
        <predicate id="cataloguePrefixAndFirstOrLastNotCompany">
            <all>
                <predicate id="CATALOGUEPREFIX"/>
                <predicate id="firstOrLast"/>
                <predicate><not><predicate id="COMPANYSUFFIX"/></not></predicate>
            </all>
        </predicate>
        
        <!-- Scoring -->
        <score predicate="categoryOrKeyword">100</score>
        <score predicate="gBanger">-500</score>
        <score predicate="companyNotPerson">200</score>
        <score predicate="firstAndLastNameAndCompanySuffix">200</score>
        <score predicate="PRIOCOMPANYNAME">30</score>
        <score predicate="geoExact">-500</score>
        <score predicate="ORGNR">120</score>
        <score predicate="PHONENUMBER">200</score>
        <score predicate="allNotsAndFirstOrLastAndGeo">-500</score>
        <score predicate="cataloguePrefixAndFirstOrLastNotCompany">-500</score>
        <score predicate="NEWSPREFIX">-500</score>
        <score predicate="TNS">-500</score>
        <score predicate="EXACTFIRST">-500</score>
    </rule>

    <rule id="globalEnrichment">
        
        <!-- private predicates -->
        <predicate id="ppp">
            <any>
                <predicate id="KEYWORD"/>
                <predicate id="CATEGORY"/>
                <predicate id="FIRSTNAME"/>
                <predicate id="LASTNAME"/>
                <predicate id="COMPANYNAME"/>
            </any>
        </predicate>
        <predicate id="englishNotWiki">
            <all>
                <predicate id="ENGLISHWORDS"/>
                <predicate><not><predicate id="WIKIPEDIA"/></not></predicate>
            </all>
        </predicate>
        
        <!-- Scoring -->
        <score predicate="englishNotWiki">90</score>
        <score predicate="ppp">-500</score>
    </rule>
    
    <rule id="picSearch">
        
        <!-- private predicates -->
        <predicate id="exactWikiWithoutPrefixes">
            <all>
                <predicate id="EXACTWIKI"/>
                <predicate>
                    <none>
                        <predicate id="EXACTCOMPANYNAME"/>
                        <predicate id="CATALOGUEPREFIX"/>
                        <predicate id="WEATHERPREFIX"/>
                        <predicate id="PICTUREPREFIX"/>
                        <predicate id="NEWSPREFIX"/>
                        <predicate id="WIKIPEDIAPREFIX"/>
                    </none>
                </predicate>
            </all>
        </predicate>
        
        <!-- Scoring -->
        <score predicate="PICTUREPREFIX">550</score>
        <score predicate="geoExact">-600</score>
    </rule>
    
    <rule id="wikipedia">
        
        <!-- private predicates -->
        <predicate id="wikiAndCompany">
            <all>
                <predicate id="PRIOCOMPANYNAME"/>
                <predicate id="WIKIPEDIA"/>
            </all>
        </predicate>
        <predicate id="wikiAndCompanyWithoutOtherPrefixes">
            <all>
                <predicate id="wikiAndCompany"/>
                <predicate>
                    <none>
                        <predicate id="CATALOGUEPREFIX"/>
                        <predicate id="WEATHERPREFIX"/>
                        <predicate id="PICTUREPREFIX"/>
                        <predicate id="NEWSPREFIX"/>
                    </none>
                </predicate>
            </all>
        </predicate>
        <predicate id="wikiAndWikiPrefix">
            <all>
                <predicate id="WIKIPEDIAPREFIX"/>
                <predicate id="WIKIPEDIA"/>
            </all>
        </predicate>
        
        <!-- Scoring -->
        <score predicate="wikiAndWikiPrefix">400</score>
        <score predicate="wikiAndCompanyWithoutOtherPrefixes">-500</score>
        <score predicate="EXACTWIKI">300</score>
        <score predicate="firstAndLastName">-220</score>
        <score predicate="geoExact">-220</score>
        <score predicate="TNS">-220</score>
    </rule>
    
    <rule id="news">
        
        <!-- private predicates -->
        
        <predicate id="pp">
            <all>
                <predicate id="firstAndLastName"/>
                <predicate id="EXACTWIKI"/>
                <predicate><not><predicate id="COMPANYSUFFIX"/></not></predicate> <!-- and not big sites -->
            </all>
        </predicate>
        <predicate id="priocompanyNameWithoutFirstNameAndGeo">
            <all>
                <predicate id="PRIOCOMPANYNAME"/>
                <predicate>
                    <none>
                        <predicate id="FIRSTNAME"/>
                        <predicate id="geo"/>
                    </none>
                </predicate>
            </all>
        </predicate>
        <predicate id="categoryOrKeywordWithoutNewsPrefix">
            <all>
                <predicate id="categoryOrKeyword"/>
                <predicate><not><predicate id="NEWSPREFIX"/></not></predicate> 
            </all>
        </predicate>
        <predicate id="companyNameWithoutPrioCompanyName">
            <all>
                <predicate id="COMPANYNAME"/>
                <predicate><not><predicate id="PRIOCOMPANYNAME"/></not></predicate> 
            </all>
        </predicate>
        <predicate id="geoExactOrWhiteBoost">
            <any>
                <predicate id="geoExact"/>
                <predicate id="whiteBoost"/>
            </any>
        </predicate>
        <predicate id="tnsOrCompanyNotPerson">
            <any>
                <predicate id="TNS"/>
                <predicate id="companyNotPerson"/>
            </any>
        </predicate>
        <predicate id="exactWikiWithoutFirstAndLastName">
            <all>
                <predicate id="EXACTWIKI"/>
                <predicate><not><predicate id="firstAndLastName"/></not></predicate> 
            </all>
        </predicate>
        <predicate id="exactFirstWithoutWikiAndCompanyName">
            <all>
                <predicate id="EXACTFIRST"/>
                <predicate>
                    <none>
                        <predicate id="COMPANYNAME"/>
                        <predicate id="EXACTWIKI"/>
                    </none>
                </predicate> 
            </all>
        </predicate>
        
        <!-- Scoring -->
        <score predicate="priocompanyNameWithoutFirstNameAndGeo">400</score>
        <score predicate="pp">300</score>
        <score predicate="NEWSPREFIX">400</score>
        <score predicate="ALWAYSTRUE">100</score>
        <score predicate="categoryOrKeywordWithoutNewsPrefix">-550</score>
        <score predicate="companyNameWithoutPrioCompanyName">-20</score>
        <score predicate="geoExactOrWhiteBoost">-20</score>
        <score predicate="tnsOrCompanyNotPerson">-20</score>
        <score predicate="CATALOGUEPREFIX">-500</score>
        <score predicate="exactWikiWithoutFirstAndLastName">-20</score>
        <score predicate="WIKIPEDIAPREFIX">-500</score>
        <score predicate="WEATHERPREFIX">-500</score>
        <score predicate="englishWordsWithoutNorwegianStuff">-500</score>
        <score predicate="exactFirstWithoutWikiAndCompanyName">-500</score>
        
    </rule>
    
    <rule id="tv">
        
        <!-- private predicates -->
        <predicate id="otherPrefTv">
            <any>
                <predicate id="CATALOGUEPREFIX"/>
                <predicate id="WEATHERPREFIX"/>
                <predicate id="PICTUREPREFIX"/>
                <predicate id="WIKIPEDIAPREFIX"/>
            </any>
        </predicate>
        <predicate id="notGeo">
            <not><predicate id="geo"/></not>
        </predicate>
        
        <!-- Scoring -->
        <score predicate="otherPrefTv">-500</score>
        <score predicate="TVPREFIX">1000</score>
        <score predicate="notGeo">500</score>
        <score predicate="NEWSPREFIX">-420</score>
        <score predicate="categoryOrKeyword">-420</score>
        <score predicate="TNS">-500</score>
    </rule>
    
    <rule id="globalSearch">
        
        <!-- private predicates -->
        
        <!-- Scoring -->
        <score predicate="englishWordsWithoutNorwegianStuff">100</score>
    </rule>
    
    <rule id="weather">
        
        <!-- private predicates -->
        <predicate id="weatherPrefixAndGeo">
            <any>
                <predicate id="WEATHERPREFIX"/>
                <predicate id="geo"/>
            </any>
        </predicate>
        <predicate id="allCompanyAndPresonHitsWithoutGeo">
            <all>
                <predicate><not><predicate id="geo"/></not></predicate> 
                <predicate>
                    <any>
                        <predicate id="FULLNAME"/>
                        <predicate id="KEYWORD"/>
                        <predicate id="CATEGORY"/>
                        <predicate id="COMPANYNAME"/>
                    </any>
                </predicate>
            </all>
        </predicate>
        
        <!-- Scoring -->
        <score predicate="weatherPrefixAndGeo">400</score>
        <score predicate="geoExact">500</score>
        <score predicate="allCompanyAndPresonHitsWithoutGeo">-500</score>
        <score predicate="TNS">-500</score>
    </rule>
    
    <rule id="mathExpression">
        
        <!-- private predicates -->
        
        <!-- Scoring -->
        <score predicate="MATHPREDICATE">500</score>
    </rule>
    
</rules>
