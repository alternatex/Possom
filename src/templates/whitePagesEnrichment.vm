<html>
<head>
    <meta name="decorator" content="maindecorator">
</head>

<body bgcolor="#eeeeff">

#if ($result.hitCount > 0)
    <content tag="whitePagesEnrichment">

        <table class="enrichment" cellspacing="0" cellpadding="0">
            #if ($result.hitCount != 1)
                <tr>
                    <td colspan="2" class="title"><a href="?c=w&amp;q=$queryHTMLEscaped">$text.getMessage($locale, "hitsOnWhiteEnrichment", $result.hitCount, $queryHTMLEscaped)</a></td>
                </tr>
            #else
                <tr>
                    #foreach( $item in $result.results )
                    <td colspan="2" class="title"><a href="?c=wip&amp;q=$query&amp;personId=$item.getField('recordid')&amp;personId_x=$hashGenerator.generateMD5($item.getField('recordid'))">$text.getMessage($locale, "oneHitOnWhiteEnrichment")</a></td>
                    #end
                </tr>
            #end
            <tr>
                <td valign="top" class="img">
                #if ($result.results.size() == 1) #foreach( $item in $result.results ) <a href="?c=wip&amp;q=$query&amp;personId=$item.getField('recordid')&amp;personId_x=$hashGenerator.generateMD5($item.getField('recordid'))"> #end #else <a href="?c=w&amp;q=$query"> #end
                    <img src="../images/enrichment/wp.jpg" alt="Personer" /></a>
                </td>
                <td width="500" class="enrich_text">
                    <table class="testtable" cellspacing="0">
                    #foreach( $item in $result.results )
                        #if ($velocityCount <= 3)
                            <tr>
                                <td valign="top"><a href="?c=wip&amp;q=$query&amp;personId=$item.getField('recordid')&amp;personId_x=$hashGenerator.generateMD5($item.getField('recordid'))">$!item.getField('wpfornavn') $!item.getField('wpmellomnavn') $!item.getField('wpetternavn')</a></td>                                
                                <td class="enrich_space"></td>
                                <td valign="top">
                                    #if ($item.getField('wpadresse'))
                                        $!item.getField('wpadresse'),
                                    #end
                                    $!item.getField('ywpostnr')
                                    $!item.getField('ywpoststed')
                                </td>
                                #if ($item.getField('wptelefon'))
                                    <td class="enrich_space"></td>
                                    <td valign="top" style="white-space: nowrap;">$item.getField('wptelefon')</td>
                                #end
                                #if ($item.getField('wpmobiltelefon'))
                                    <td class="enrich_space"></td>
                                    <td valign="top" style="white-space: nowrap;">$item.getField('wpmobiltelefon')</td>
                                #end
                            </tr>
                        #end
                    #end
                    </table>
                </td>
            </tr>
        </table>

    </content>
#end

</body>
</html>
