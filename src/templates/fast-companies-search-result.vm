<html>
	<head>
		<meta name="decorator" content="maindecorator">
	</head>


<body>
    <b>Companies Results</b>

    <content tag="companies-results">

        <table id="company_result" cellspacing="2">
            #foreach ($item in $result.getCompaniesResults())
                <tr>
                    <td colspan="2"><b>$item.companyName</b></td>
                </tr>
                <tr>
                    <td>$item.visitingAddress , $item.visitingZip $item.visitingCity</td>
                    <td>$!{item.phoneNumber}</td>
                </tr>
                <tr><td colspan="2" class="company_result_borderline">&nbsp;</td></tr>
            #end
        </table>

    </content>

    <content tag="more-results">           
        #set ($tab = "y")
        #parse("verbosePager.vm")
    </content>


</body>
</html>