<html>
<head>
    <meta name="decorator" content="maindecorator">
</head>

<body>


    <content tag="greybar_magic">
        #searchBar()

        <div id="greybar_main">
            <div class="left">$text.getMessage($locale, "numberFormat", $pager.numberOfResults) treff p&#229; <b>$queryHTMLEscaped</b></div>
            <div class="right">&nbsp;</div>
        </div>
        <img src="$site.getImageDir()/pix.gif" width="100%" height="1" class="dots" alt="" />
        #set ($baseURL = "?c=d&amp;q=$query")
        #if ($result.getQuerySuggestions().size() > 0)
            <div class="spelling_suggestion">
                <img src="$site.getImageDir()/star.gif" align="middle"/>
                <span class="stavekontroll">
                    $text.getMessage($locale, "didYouMean"):
                    #foreach ($querySuggestion in $result.querySuggestions)
                        <em><a href="?c=d&amp;q=$querySuggestion.query">$querySuggestion.displayQuery</a></em>
                        #if  ($velocityCount < $result.getQuerySuggestions().size())
                          eller
                        #end
                    #end
                    ?
                </span>
            </div>
        #end
    </content>


</body>
</html>