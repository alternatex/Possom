#if($result)
<html>
	<head>
		<meta name="decorator" content="maindecorator">
	</head>


<body>

<content tag="sensis-results">
#if ($result.getTotalDocumentsAvailable() > 0)
	 ($result.getTotalDocumentsAvailable())
#end
</content>

<content tag="sensis-enriched">
  <div class="beriketVerden">
  	<a href="?s=g&q=$result.getQuery()" class="beriketTittel">TREFF I VERDEN</a><br/>
    <div class="beriketInnrykk">

#foreach( $item in $result.getResults() )

  #if ($velocityCount <= 3)
	 <a href="$item.url">$item.title</a><br/>
	#end
#end
   </div>
  </div>

</content>

<content tag="global-results">	
<p/>

<div class="sokeresultat">

<div class="boksTittel">
 $result.getTotalDocumentsAvailable() TREFF I NORGE
</div>

#foreach( $item in $result.getResults() )

	<a href="$item.url"><strong>$item.title</strong></a><br/>
	$item.summary
	<br/>
	<a class="litenUrl" href="$item.url">$item.clickUrl</a>
	<p/>
#end

<p class="kilde">Nerdscore: $result.getFetchTime() ms</p> 

    #set ($tab = "g")
    #parse("verbosePager.vm")


</content>
</body>
</html>
#end