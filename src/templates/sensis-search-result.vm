	#if($steps > 10)
		#set( $steps = 10)
	#end

	#foreach( $step in [0..$steps] )
		#set($offset = $result.getConsequtiveSearchStartsAt() + 10 * $step)
		#set($count = $step + 1)
		<a href="?c=d&q=$result.getQuery()&offset=$offset">$count</a>&nbsp;
	#end
	
	</div>
