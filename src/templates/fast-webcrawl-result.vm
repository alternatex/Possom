<html>
	<head>
		<meta name="decorator" content="maindecorator">
	</head>

    <body>

    <content tag="fast-enriched">
        <div class="enrichment">
            <a href="?q=$result.getQuery()&amp;c=d" class="enrichment_link">$result.getTotalDocumentsAvailable() Treff <b>$result.query</b> i Norge</a>
            <ul>
                #foreach( $item in $result.getResults())
                #if ($velocityCount <= 3)
                <li><a href="$item.url">$item.title</a></li>
                #end
                #end
            </ul>
        </div>
    </content>
        <content tag="search-error-fast">
            #if($result.getSearchErrorMesg())
                $result.getSearchErrorMesg()
            #end
        </content>

        <content tag="spelling-suggestions">
            #if($result.getSpellingSuggestions())
                <div class="spelling_suggestion">Mente du:
                    #foreach($suggestion in $result.getSpellingSuggestions())
                      <em><a href="?q=$suggestion">$suggestion</a>?</em>
                    #end
                </div>
            #end
        </content>

        <content tag="fast-results">
            #if ($result.getTotalDocumentsAvailable() > 0)
                <div id="search_result">
                    <dl class="search_hit_header">
                        <dt>Viser $result.getPager().getFirstHitOnPage() - #if ($result.getTotalDocumentsAvailable() < $result.getPager().getLastHitOnPage()) $result.getTotalDocumentsAvailable() #else $result.getPager().getLastHitOnPage() #end av $result.getTotalDocumentsAvailable() på <b>$result.getQuery()</b></dt>
                        <dd>#if ($result.getPager().getNumberOfPages() > 1)<a href="$baseURL&amp;offset=$result.getPager().getOffsetOfNextPage()" class="search_next_click">10 neste treff</a>#end</dd>
                    </dl>
                    #foreach( $item in $result.getResults() )
                        <p>
                            <a href="$item.url" class="search_big_url">$item.title</a>
                            <div class="search_summary">$!{item.summary}</div>
                            <a class="search_small_url" href="$item.url">$item.clickUrl</a>
                        </p>
                    #end
                    <p>Nerdscore: $result.getFetchTime() ms</p>
                </div>
            #else
                <div id="no_hits">
                    #parse("no-hits.vm")
                </div>
            #end
        </content>

        <content tag="more-results-d">
            #set ($tab = "d")
            #parse("verbosePager.vm")
        </content>
    </body>
</html>